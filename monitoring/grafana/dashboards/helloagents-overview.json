{
  "dashboard": {
    "id": null,
    "uid": "helloagents-overview",
    "title": "HelloAgents Platform - Overview",
    "tags": ["helloagents", "overview"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "30s",

    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "type": "stat",
        "title": "Service Availability (30d)",
        "targets": [{
          "expr": "sum(rate(http_requests_total{job=\"helloagents-backend\",status_code!~\"5..\"}[30d])) / sum(rate(http_requests_total{job=\"helloagents-backend\"}[30d])) * 100",
          "refId": "A"
        }],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 99, "color": "yellow"},
                {"value": 99.5, "color": "green"}
              ]
            }
          }
        }
      },

      {
        "id": 2,
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "type": "stat",
        "title": "Request Rate (req/s)",
        "targets": [{
          "expr": "sum(rate(http_requests_total{job=\"helloagents-backend\"}[5m]))",
          "refId": "A"
        }],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 2
          }
        }
      },

      {
        "id": 3,
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "type": "stat",
        "title": "P95 Latency",
        "targets": [{
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"helloagents-backend\"}[5m]))",
          "refId": "A"
        }],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 2.0, "color": "red"}
              ]
            }
          }
        }
      },

      {
        "id": 4,
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "type": "stat",
        "title": "Error Rate",
        "targets": [{
          "expr": "sum(rate(http_requests_total{job=\"helloagents-backend\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"helloagents-backend\"}[5m])) * 100",
          "refId": "A"
        }],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },

      {
        "id": 5,
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "type": "graph",
        "title": "Request Rate by Status Code",
        "targets": [{
          "expr": "sum(rate(http_requests_total{job=\"helloagents-backend\"}[5m])) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        }],
        "yaxes": [{
          "format": "reqps",
          "label": "Requests/sec"
        }],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "rightSide": true
        }
      },

      {
        "id": 6,
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "type": "graph",
        "title": "Response Time Percentiles",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"helloagents-backend\"}[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"helloagents-backend\"}[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"helloagents-backend\"}[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [{
          "format": "s",
          "label": "Response Time"
        }]
      },

      {
        "id": 7,
        "gridPos": {"x": 0, "y": 12, "w": 8, "h": 8},
        "type": "graph",
        "title": "Sandbox Executions",
        "targets": [{
          "expr": "sum(rate(sandbox_executions_total[5m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }],
        "yaxes": [{
          "format": "ops",
          "label": "Executions/sec"
        }]
      },

      {
        "id": 8,
        "gridPos": {"x": 8, "y": 12, "w": 8, "h": 8},
        "type": "graph",
        "title": "Sandbox Execution Duration (P95)",
        "targets": [{
          "expr": "histogram_quantile(0.95, rate(sandbox_execution_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "A"
        }],
        "yaxes": [{
          "format": "s",
          "label": "Execution Time"
        }]
      },

      {
        "id": 9,
        "gridPos": {"x": 16, "y": 12, "w": 8, "h": 8},
        "type": "graph",
        "title": "Sandbox Container Pool",
        "targets": [
          {
            "expr": "sandbox_pool_available",
            "legendFormat": "Available",
            "refId": "A"
          },
          {
            "expr": "sandbox_pool_in_use",
            "legendFormat": "In Use",
            "refId": "B"
          }
        ],
        "yaxes": [{
          "format": "short",
          "label": "Containers"
        }]
      },

      {
        "id": 10,
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "type": "graph",
        "title": "AI Chat Requests",
        "targets": [{
          "expr": "sum(rate(ai_chat_requests_total[5m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }],
        "yaxes": [{
          "format": "reqps",
          "label": "Requests/sec"
        }]
      },

      {
        "id": 11,
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "type": "graph",
        "title": "AI Response Time (P95)",
        "targets": [{
          "expr": "histogram_quantile(0.95, rate(ai_chat_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "A"
        }],
        "yaxes": [{
          "format": "s",
          "label": "Response Time"
        }]
      }
    ],

    "templating": {
      "list": []
    },

    "time": {
      "from": "now-1h",
      "to": "now"
    },

    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  },

  "overwrite": true
}
